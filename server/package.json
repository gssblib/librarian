{
  "name": "gssblib",
  "version": "0.0.2",
  "description": "GSSB library server v2",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "npm install && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "start-server": "npx ts-node src/main.ts",
    "start-server-dev": "npx nodemon src/main.ts",
    "sync-antolin": "npx nodemon src/sync_antolin.ts",
    "verify-labels": "npx nodemon src/verify_labels.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "config"
    ],
    "ext": "ts,js, json"
  },
  "author": "Andreas Hohmann",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@types/config": "0.0.39",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/needle": "^2.5.2",
    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/nunjucks": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "express": "^4.17.1",
    "firebase-functions-test": "^2.2.0",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@pdfme/generator": "^1.0.14",
    "@types/multer": "^1.4.7",
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "csv-parse": "^5.3.0",
    "express-jwt": "^6.1.0",
    "firebase-functions": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.0",
    "needle": "^3.0.0",
    "nodemailer": "^6.7.0",
    "nunjucks": "^3.2.3"
  }
}
